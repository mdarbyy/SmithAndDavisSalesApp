<tr>
  <td><%= link_to "#{sales_record.sales_person.first_name} #{sales_record.sales_person.last_name}", edit_sales_record_path(sales_record, return_to: request.fullpath), class: "nav-link" %></td>
  <td><%= sales_record.sell_date.strftime("%m/%d/%Y") %></td>
  <td>$<%= sales_record.amount_sold %></td>
  <td><%= sales_record.items_sold %></td>
  <td><%= sales_record.project_hours %></td>
  <td><%= sales_record.sales_floor_hours %></td>
  <td>
    <button class="btn btn-delete"
      type="button"
      data-entity="sales_records"
      data-entity-name="Sales Record"
      data-id="<%= sales_record.id %>"
      onclick="deleteEntity(event)">
      Delete
    </button>
  </td>
</tr>